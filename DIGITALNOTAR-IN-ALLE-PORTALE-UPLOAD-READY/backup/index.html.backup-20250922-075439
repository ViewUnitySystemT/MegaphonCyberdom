    <script>
// ===============================
// üß† Maintain√© AI + Floating UI + PWA + Telemetrie + OTLP + WebAuthn
// ===============================
(function(){
  const LS = window.localStorage;
  const qs = (s, r=document)=>r.querySelector(s);
  const qsa = (s, r=document)=>Array.from(r.querySelectorAll(s));
  const on = (el, ev, fn)=> el && el.addEventListener(ev, fn);
  const log = (...args)=> console.log('üèõÔ∏è', ...args);

  // ----- Global Suche -----
  function applySearchFilter(term){
    const norm = (term||'').trim().toLowerCase(); let visible=0;
    qsa('.portal-card').forEach(card=>{ const text=card.innerText.toLowerCase(); const show=!norm||text.includes(norm); card.style.display=show?'':'none'; if(show) visible++; });
    const meta = qs('#searchMeta'); if(meta) meta.textContent = norm?`${visible} Treffer`:'Tipp: Dr√ºcke "/" zum Fokussieren ‚Ä¢ Filtert Karten, Titel & Beschreibungen in Echtzeit';
  }
  on(qs('#searchBtn'),'click',()=>applySearchFilter(qs('#searchInput').value));
  on(qs('#searchInput'),'input', e=> applySearchFilter(e.target.value));
  on(document,'keydown', e=>{ if(e.key === '/'){ e.preventDefault(); qs('#searchInput')?.focus(); }});

  // ----- Telemetry Opt‚ÄëIn -----
  const CONSENT_KEY='telemetryConsent'; const banner=qs('#telemetryBanner'); const consent=LS.getItem(CONSENT_KEY);
  if(!consent){ banner?.classList.add('active'); }
  on(qs('#btnTelemetryAllow'),'click',()=>{ LS.setItem(CONSENT_KEY,'granted'); banner?.classList.remove('active'); syncToggle(); log('üîé Telemetry: granted'); });
  on(qs('#btnTelemetryDecline'),'click',()=>{ LS.setItem(CONSENT_KEY,'denied'); banner?.classList.remove('active'); syncToggle(); log('üîé Telemetry: declined'); });
  function track(eventName,payload={}){ if(LS.getItem(CONSENT_KEY)!=='granted') return; window.__telemetryQueue=window.__telemetryQueue||[]; window.__telemetryQueue.push({ts:Date.now(),event:eventName,payload}); }

  // ----- Floating UI -----
  const panel=qs('#floatPanel'); const launcher=qs('#floatLauncher'); on(launcher,'click',()=>panel.classList.toggle('show'));
  qsa('.fp-tabs button').forEach(btn=> on(btn,'click',()=>{ qsa('.fp-tabs button').forEach(b=>b.classList.remove('active')); btn.classList.add('active'); const tab=btn.dataset.tab; qsa('[data-pane]').forEach(p=>{ p.hidden = p.dataset.pane!==tab; }); }));
  on(qs('[data-action="focusSearch"]'),'click',()=> qs('#searchInput')?.focus());
  on(qs('[data-action="revealHidden"]'),'click',()=>{ const h=qs('#hiddenTools'); h?.classList.add('revealed'); h?.setAttribute('aria-hidden','false'); LS.setItem('featuresAll','1'); track('FEATURES_UNLOCKED'); });

  // ----- Settings (Privacy Center) -----
  function syncToggle(){ const t=qs('#toggleTelemetry'); if(!t) return; t.checked = LS.getItem(CONSENT_KEY)==='granted'; }
  syncToggle();
  on(qs('#toggleTelemetry'),'change', e=>{ LS.setItem(CONSENT_KEY, e.target.checked?'granted':'denied'); log('üîé Telemetry toggle =>', e.target.checked); });
  on(qs('[data-action="exportData"]'),'click',()=>{
    const data = { telemetryQueue: window.__telemetryQueue||[], settings:{ consent: LS.getItem(CONSENT_KEY), featuresAll: LS.getItem('featuresAll') } };
    const blob = new Blob([JSON.stringify(data,null,2)], {type:'application/json'});
    const url = URL.createObjectURL(blob); const a=document.createElement('a'); a.href=url; a.download='digitalnotar-privacy-export.json'; a.click(); URL.revokeObjectURL(url);
  });
  on(qs('[data-action="deleteData"]'),'click',()=>{ ['featuresAll',CONSENT_KEY].forEach(k=>LS.removeItem(k)); window.__telemetryQueue=[]; caches.keys().then(keys=> keys.forEach(k=>caches.delete(k))); alert('Lokale Daten gel√∂scht.'); syncToggle(); });

  // ----- OTLP/HTTP Export (JSON simplified) -----
  on(qs('[data-action="sendTraces"]'),'click', async ()=>{
    try{
      const endpoint = qs('#otlpEndpoint')?.value?.trim(); if(!endpoint) return alert('Bitte Endpoint angeben.');
      const payload = {resource:{service:'digitalnotar.in'}, traces: (window.__telemetryQueue||[]) };
      const res = await fetch(endpoint, {method:'POST', headers:{'content-type':'application/json'}, body: JSON.stringify(payload)});
      alert(res.ok? 'Traces gesendet.' : `Fehler: ${res.status}`);
    }catch(e){ alert('Fehler beim Senden: '+ e.message); }
  });
  on(qs('[data-action="downloadTraces"]'),'click',()=>{
    const blob = new Blob([JSON.stringify(window.__telemetryQueue||[],null,2)], {type:'application/json'});
    const url = URL.createObjectURL(blob); const a=document.createElement('a'); a.href=url; a.download='digitalnotar-traces.json'; a.click(); URL.revokeObjectURL(url);
  });

  // ----- PWA Install Prompt -----
  let deferredPrompt=null; const iBanner=qs('#installBanner');
  window.addEventListener('beforeinstallprompt', (e)=>{ e.preventDefault(); deferredPrompt=e; iBanner.classList.add('show'); });
  on(qs('#btnInstallNow'),'click', async ()=>{ if(!deferredPrompt) return; deferredPrompt.prompt(); const { outcome } = await deferredPrompt.userChoice; iBanner.classList.remove('show'); track('PWA_INSTALL', {outcome}); deferredPrompt=null; });

  // ----- Service Worker for Offline -----
  async function registerInlineServiceWorker(){ if(!('serviceWorker' in navigator)) return; const sw=`self.addEventListener('install',e=>{e.waitUntil(caches.open('dn-static-v2').then(c=>c.addAll(['./'])))}); self.addEventListener('fetch',e=>{e.respondWith(caches.match(e.request).then(r=>r||fetch(e.request).then(res=>{const copy=res.clone(); caches.open('dn-dyn-v2').then(c=>c.put(e.request,copy)); return res;}).catch(()=>caches.match('./'))))});`; const blob=new Blob([sw],{type:'text/javascript'}); const url=URL.createObjectURL(blob); try{ await navigator.serviceWorker.register(url);}catch(e){ console.warn('SW fail', e);} }
  registerInlineServiceWorker();

  // ----- Offline Badge -----
  const offlineBadge = qs('#offlineBadge'); function updateOnline(){ if(navigator.onLine){ offlineBadge?.classList.remove('show'); } else { offlineBadge?.classList.add('show'); } } ['online','offline'].forEach(ev=> on(window, ev, updateOnline)); updateOnline();

  // ----- Maintain√© AI Heartbeat -----
  const MaintainAI={ heartbeatMs:15000, tries:0, baseDelay:1000, maxBackoff:5*60*1000, healthy:false, auditTrail:[], ensureAPI(){ if(!window.universalAIAPI){ window.universalAIAPI={ auditTrail:{events:[]}, health:()=>Promise.resolve({status:'degraded'})}; } return window.universalAIAPI; }, async ping(){ const api=this.ensureAPI(); try{ const res= await (api.health? api.health(): Promise.resolve({status:'ok'})); this.healthy= !!res; this.tries=0; this.audit('HEARTBEAT', res.status||'ok'); }catch(e){ this.healthy=false; this.audit('HEARTBEAT_FAIL', e?.message||'error'); this.retry(); } }, retry(){ this.tries++; const d=Math.min(this.maxBackoff, this.baseDelay*2**(this.tries-1)); setTimeout(()=>this.ping(), d); }, audit(type,detail){ const evt={ts:Date.now(), type, detail}; this.auditTrail.push(evt); window.universalAIAPI?.auditTrail?.events.push({timestamp:new Date(), type, detail, severity:'info'}); track('AI_'+type,{detail}); }, start(){ this.ping(); setInterval(()=>this.ping(), this.heartbeatMs); document.addEventListener('visibilitychange', ()=>{ if(!document.hidden) this.ping(); }); } }; MaintainAI.start();

  // ----- WebAuthn (Demo - ben√∂tigt HTTPS & Server f√ºr echte Challenges) -----
  async function webauthnRegister(){
    if(!('credentials' in navigator)) return alert('WebAuthn nicht unterst√ºtzt.');
    try{
      const publicKey = { challenge: Uint8Array.from(self.crypto.getRandomValues(new Uint8Array(32))), rp: { name: 'DIGITALNOTAR.IN' }, user: { id: Uint8Array.from(self.crypto.getRandomValues(new Uint8Array(16))), name: 'user@example.com', displayName: 'User' }, pubKeyCredParams: [{type:'public-key', alg:-7}], authenticatorSelection:{ userVerification:'preferred' } };
      const cred = await navigator.credentials.create({ publicKey });
      LS.setItem('webauthnRegistered','1'); track('WEBAUTHN_REGISTER'); alert('WebAuthn registriert (Demo).');
    }catch(e){ alert('WebAuthn Fehler: '+ e.message); }
  }
  async function webauthnLogin(){
    if(!('credentials' in navigator)) return alert('WebAuthn nicht unterst√ºtzt.');
    try{
      const publicKey = { challenge: Uint8Array.from(self.crypto.getRandomValues(new Uint8Array(32))), userVerification: 'preferred' };
      const assertion = await navigator.credentials.get({ publicKey });
      track('WEBAUTHN_LOGIN'); alert('WebAuthn Login (Demo).');
    }catch(e){ alert('WebAuthn Fehler: '+ e.message); }
  }
  on(qs('[data-action="webauthnRegister"]'),'click', webauthnRegister);
  on(qs('[data-action="webauthnLogin"]'),'click', webauthnLogin);

  // ----- Portal click tracking -----
  qsa('.portal-link').forEach(link=> on(link,'click',()=>{ const portalName=link.closest('.portal-card')?.querySelector('h3')?.textContent||'Unbekannt'; track('PORTAL_ACCESS',{portal:portalName}); }));

  // ----- KRITISCHE BUG-REPARATUR: Globale Suche System -----
  const searchInput = qs('#searchInput');
  const searchMeta = qs('#searchMeta');
  let searchTimeout = null;
  let searchAbortController = null;
  
  // Cleanup function f√ºr Event-Listener
  function cleanupSearchListeners() {
    if (searchTimeout) {
      clearTimeout(searchTimeout);
      searchTimeout = null;
    }
    if (searchAbortController) {
      searchAbortController.abort();
      searchAbortController = null;
    }
  }
  
  // Keyboard shortcuts mit Cleanup
  document.addEventListener('keydown', function(e) {
    // "/" zum Fokussieren der globalen Suche
    if (e.key === '/' && !e.ctrlKey && !e.altKey && !e.metaKey) {
      if (document.activeElement !== searchInput && searchInput) {
        e.preventDefault();
        searchInput.focus();
        searchInput.select();
        log('üîç Suche fokussiert via "/" Tastatur-Shortcut');
      }
    }
    
    // "Esc" zum Leeren der Suche
    if (e.key === 'Escape' && searchInput) {
      cleanupSearchListeners();
      searchInput.value = '';
      searchInput.blur();
      resetSearchResults();
      log('üîç Suche geleert via "Esc" Tastatur-Shortcut');
    }
  });

  // Reset search results function
  function resetSearchResults() {
    const allCards = qsa('.portal-card');
    allCards.forEach(card => {
      card.style.display = 'block';
      card.classList.remove('search-highlight');
    });
    if (searchMeta) {
      searchMeta.textContent = 'Tipp: Dr√ºcke "/" zum Fokussieren ‚Ä¢ Filtert Karten, Titel & Beschreibungen in Echtzeit';
    }
  }

  // Advanced search with debounce and API integration
  if (searchInput) {
    on(searchInput, 'input', function() {
      const query = this.value.toLowerCase().trim();
      
      // Cleanup previous search
      cleanupSearchListeners();
      
      // Debounce search
      searchTimeout = setTimeout(async () => {
        if (query === '') {
          resetSearchResults();
          return;
        }
        
        log('üîç Erweiterte Suche gestartet:', query);
        
        // Try API search first
        try {
          searchAbortController = new AbortController();
          const response = await fetch(`/api/search?q=${encodeURIComponent(query)}`, {
            signal: searchAbortController.signal,
            headers: {
              'Content-Type': 'application/json',
              'Accept': 'application/json'
            }
          });
          
          if (response.ok) {
            const data = await response.json();
            if (data.status === 'success') {
              displayApiSearchResults(data.results, query);
              log('üîç API-Suche erfolgreich:', `${data.count} Ergebnisse`);
              return;
            }
          }
        } catch (error) {
          if (error.name !== 'AbortError') {
            console.warn('üîç API-Suche fehlgeschlagen, verwende lokale Suche:', error.message);
          }
        }
        
        // Fallback: Local search
        performLocalSearch(query);
        
      }, 250); // 250ms debounce
    });
    
    // Suche-Button Event
    const searchBtn = qs('#searchBtn');
    if (searchBtn) {
      on(searchBtn, 'click', function() {
        searchInput.focus();
        searchInput.select();
        log('üîç Suche-Button geklickt');
      });
    }
  }

  // API search results display
  function displayApiSearchResults(results, query) {
    const allCards = qsa('.portal-card');
    let visibleCount = 0;
    
    // Hide all cards first
    allCards.forEach(card => {
      card.style.display = 'none';
      card.classList.remove('search-highlight');
    });
    
    // Show matching cards
    results.forEach(result => {
      const matchingCard = Array.from(allCards).find(card => {
        const title = card.querySelector('h3')?.textContent?.toLowerCase() || '';
        return title.includes(result.title.toLowerCase());
      });
      
      if (matchingCard) {
        matchingCard.style.display = 'block';
        matchingCard.classList.add('search-highlight');
        visibleCount++;
      }
    });
    
    // Update search meta
    if (searchMeta) {
      searchMeta.textContent = `üîç ${visibleCount} API-Ergebnisse f√ºr "${query}" ‚Ä¢ Dr√ºcke "Esc" zum Zur√ºcksetzen`;
    }
    
    log('üîç API-Suchergebnisse angezeigt:', `${visibleCount} Karten`);
  }

  // Local search fallback
  function performLocalSearch(query) {
    const allCards = qsa('.portal-card');
    let visibleCount = 0;
    
    allCards.forEach(card => {
      const title = card.querySelector('h3')?.textContent?.toLowerCase() || '';
      const description = card.querySelector('.description')?.textContent?.toLowerCase() || '';
      const category = card.querySelector('.category')?.textContent?.toLowerCase() || '';
      
      if (title.includes(query) || description.includes(query) || category.includes(query)) {
        card.style.display = 'block';
        card.classList.add('search-highlight');
        visibleCount++;
      } else {
        card.style.display = 'none';
        card.classList.remove('search-highlight');
      }
    });
    
    // Update search meta
    if (searchMeta) {
      searchMeta.textContent = `üîç ${visibleCount} lokale Ergebnisse f√ºr "${query}" ‚Ä¢ Dr√ºcke "Esc" zum Zur√ºcksetzen`;
    }
    
    log('üîç Lokale Suche abgeschlossen:', `${visibleCount} Ergebnisse gefunden`);
  }

  // ----- Navigation-Reparatur f√ºr Home-Button -----
  window.addEventListener('pageshow', function() {
    console.log('üîÑ Hauptseite geladen - Suche neu initialisiert');
    // Suche nach Navigation neu initialisieren
    if (searchInput) {
      searchInput.focus();
      console.log('üîç Suche nach Navigation fokussiert');
    }
  });

  // Cache-Busting f√ºr alle Links um sicherzustellen, dass die neueste Version geladen wird
  qsa('a[href*=".html"]').forEach(link => {
    if (!link.href.includes('?')) {
      link.addEventListener('click', function(e) {
        e.preventDefault();
        const originalUrl = this.getAttribute('href');
        const url = new URL(originalUrl, window.location.origin);
        url.searchParams.set('t', Date.now());
        console.log('üîó Link mit Cache-Busting:', url.toString());
        window.location.href = url.toString();
      });
    }
  });

  log('‚úÖ Schwebende UI/UX aktiv ‚Ä¢ Privacy Center ‚Ä¢ OTLP Export ‚Ä¢ PWA Install ‚Ä¢ WebAuthn Demo bereit ‚Ä¢ Globale Suche mit / Shortcut ‚Ä¢ Navigation repariert');
})();

